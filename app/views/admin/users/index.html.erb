<h1>Listing admin_users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Created at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_users.each do |admin_user| %>
  <tr>
    <td><%= admin_user.email %></td>
    <td><%= admin_user.created_at %></td>
    <td><%= link_to 'Show', [:admin, admin_user] %></td>
    <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
    <td><%= link_to 'Destroy', [:admin, admin_user], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_admin_user_path %>
